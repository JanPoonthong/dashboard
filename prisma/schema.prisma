generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Analytics {
  id        Int      @id @default(autoincrement())
  createdAt DateTime
  year1     Int
  year2     Int
  year3     Int
  year4     Int
}

model Student {
  id        Int    @id @default(autoincrement())
  studentID Int    @unique
  startTime String
  endTime   String
}

model User {
  id            String    @id
  name          String    @unique
  password      String?
}
